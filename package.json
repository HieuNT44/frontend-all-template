{
  "name": "tomosia-frontend-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow npm",
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "clean": "rm -rf .next && rm -rf node_modules",
    "changelog": "conventional-changelog -p atom -i CHANGELOG.md -s -r 0",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "prepare": "lefthook install",
    "generate:presets": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/generate-theme-presets.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.10.0",
    "@tanstack/react-query": "^5.90.8",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.453.0",
    "next": "^15.1.6",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.66.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^2.15.4",
    "simple-icons": "^15.20.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.16.0",
    "@next/bundle-analyzer": "^15.1.6",
    "@tailwindcss/postcss": "^4.1.17",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.26.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^15.15.0",
    "knip": "^5.30.0",
    "lefthook": "^1.11.1",
    "only-allow": "^1.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.5",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
